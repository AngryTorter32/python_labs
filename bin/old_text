def top_n(d, n):
    uni = set(d)
    uni = list(uni)
    uni = sorted(uni)
    kol = []
    fin = {}
    for i in range(len(uni)):
        kol.append(d.count(uni[i]))
    for i in range(len(uni)):
        fin.update({uni[i]:kol[i]})
    s_fin = dict(sorted(fin.items(), key = lambda item: (-item[1], item[0])))
    s_fin = fin
    for i in range(len(s_fin) - n):
        s_fin.popitem()
    return s_fin